{
  "name": "angular2-google-maps-example",
  "version": "1.0.0",
  "description": "Angular 2 Google maps example after update to RC1",
  "main": "index.js",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "typings install",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "format": "clang-format -i -style=file --glob=app/**/*.ts"
  },
  "author": "Carl",
  "license": "ISC",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carl09/angular2-google-maps-example.git"
  },
  "keywords": [
    "angular2",
    "google-maps"
  ],
  "dependencies": {
    "@angular/common": "^2.0.0-rc.1",
    "@angular/compiler": "^2.0.0-rc.1",
    "@angular/core": "^2.0.0-rc.1",
    "@angular/platform-browser": "^2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
    "angular2-google-maps": "git+https://github.com/carl09/angular2-google-maps.git#issue293",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "^0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.1.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "tslint": "^3.10.1",
    "clang-format": "^1.0.35"
  }
}
